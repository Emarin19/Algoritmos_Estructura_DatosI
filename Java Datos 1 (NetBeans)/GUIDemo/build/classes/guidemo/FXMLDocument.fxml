<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="700.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="guidemo.FXMLDocumentController">
   <children>
      <VBox layoutX="23.0" layoutY="24.0" prefHeight="243.0" prefWidth="186.0" spacing="10.0">
         <children>
            <Label text="CheckBox Object">
               <font>
                  <Font name="System Bold Italic" size="19.0" />
               </font>
            </Label>
            <CheckBox fx:id="peperoniCheckBox" mnemonicParsing="false" text="Peperoni" />
            <CheckBox fx:id="piñaCheckBox" mnemonicParsing="false" text="Piña" />
            <CheckBox fx:id="biconCheckBox" mnemonicParsing="false" text="Bicon" />
            <Button mnemonicParsing="false" onAction="#MSpizzaselected" text="Ordenar Pizza" />
            <Label fx:id="pizzaOrderLabel" alignment="TOP_LEFT" contentDisplay="TOP" prefHeight="92.0" prefWidth="186.0" text="Label" />
         </children>
      </VBox>
   </children>
</AnchorPane>
